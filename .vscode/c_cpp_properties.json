                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "PLATFORM_WEB",
                "GRAPHICS_API_OPENGL_ES2"
            ],
            "compilerPath": "C:/cpp/lib/raylib-web/emsdk/upstream/emscripten/emcc.bat",
            "cStandard": "c99",
            "cppStandard": "c++14",
            "intelliSenseMode": "gcc-x64"
